<h1>Articles#new</h1>
<p>Find me in app/views/articles/new.html.erb</p>

<%= form_with model: @article do |f| %>

  <%= f.label :title, 'タイトル', class: '' %>
  <%= f.text_field :title %>

  <%= f.label :bady, '本文' %>
  <%= f.text_area :bady %>

  <%= f.label :image, '画像' %>
  <%= f.attachment_field :image %>

  <%= f.submit '投稿' %>

<% end %>